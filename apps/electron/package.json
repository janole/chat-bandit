{
    "name": "electron-app",
    "private": true,
    "productName": "Chat Bandit",
    "version": "0.2.17",
    "description": "An all-in-one AI assistant for your desktop.",
    "main": "./out/main/index.js",
    "author": "Jan Ole Suhr <ole@janole.com> (https://janole.com/)",
    "type": "module",
    "homepage": "./",
    "scripts": {
        "format": "prettier --write .",
        "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
        "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
        "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
        "typecheck": "npm run typecheck:node && npm run typecheck:web",
        "start": "electron-vite preview",
        "dev": "electron-vite dev",
        "build": "npm run typecheck && electron-vite build",
        "postinstall": "electron-builder install-app-deps",
        "build:unpack": "npm run build && electron-builder --dir",
        "build:win": "npm run build && electron-builder --win",
        "build:mac": "electron-vite build && electron-builder --mac",
        "build:linux": "electron-vite build && electron-builder --linux",
        "version": "echo $(echo $npm_package_version)",
        "sbom": "node scripts/license-checker.js"
    },
    "dependencies": {
        "@electron-toolkit/preload": "^3.0.1",
        "@electron-toolkit/utils": "^3.0.0",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@fontsource/instrument-serif": "^5.2.6",
        "@fontsource/roboto": "^5.2.6",
        "@google/genai": "^1.4.0",
        "@icons-pack/react-simple-icons": "^13.1.0",
        "@mui/icons-material": "^7.1.0",
        "@mui/material": "^7.1.0",
        "@uiw/codemirror-extensions-langs": "^4.23.13",
        "@uiw/codemirror-theme-github": "^4.23.13",
        "@uiw/react-codemirror": "^4.23.8",
        "autosuggest-highlight": "^3.3.4",
        "electron-store": "^8.2.0",
        "electron-updater": "^6.1.7",
        "glob": "^11.0.2",
        "js-tiktoken": "^1.0.18",
        "lucide-react": "^0.474.0",
        "mark.js": "^8.11.1",
        "marked": "^15.0.12",
        "node-llama-cpp": "^3.8.1",
        "ollama": "^0.5.15",
        "openai": "^4.87.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-markdown": "^9.1.0",
        "react-resizable-panels": "^2.1.7",
        "react-router-dom": "^6.30.0",
        "rehype-katex": "^7.0.1",
        "rehype-raw": "^7.0.0",
        "rehype-sanitize": "^6.0.0",
        "remark-gfm": "^4.0.0",
        "remark-math": "^6.0.0",
        "semver": "^7.7.2",
        "ulid": "^3.0.0",
        "use-broadcast-ts": "^2.0.1",
        "usehooks-ts": "^3.1.1",
        "write-file-atomic": "^6.0.0",
        "zustand": "^5.0.3"
    },
    "devDependencies": {
        "@electron-toolkit/eslint-config-prettier": "^2.0.0",
        "@electron-toolkit/eslint-config-ts": "^2.0.0",
        "@electron-toolkit/tsconfig": "^1.0.1",
        "@types/node": "^20.14.8",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.21",
        "electron": "34.5.8",
        "electron-builder": "^25.1.8",
        "electron-vite": "^3.0.0",
        "eslint": "^8.57.1",
        "eslint-plugin-react": "^7.34.3",
        "postcss": "^8.5.5",
        "prettier": "^3.3.2",
        "tailwindcss": "^3.4.17",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.7.3",
        "vite": "^6.2.0"
    },
    "typings": "preload/index.d.ts"
}